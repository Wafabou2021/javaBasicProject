package com.syntax.class07;

public class Task2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
				

		int num = 1;
		
		while(num<=100) {
			System.out.println(num +" ");
			num+=2;
		}


	
	int e=1;
	while (e<=30) {
		if (e%2==0) {
		System.out.println(e+" ");
		e++;
	}
	
	
	
	
	
	
	
	
	
		
	}
	
	
	
	}}
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
