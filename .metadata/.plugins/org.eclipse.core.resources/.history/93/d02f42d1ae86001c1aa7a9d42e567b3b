package com.syntax.class07;

public class Task2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		printNumbers(100);	
		}
		
		public static void printNumbers(int num)
		{
			if(num > 0)
			{
				System.out.print(num +" "); 
				printNumbers(num - 1);
			}	
		}
	}
			
			
			
			
			
			
			
			
			
			
			
			
			
			
			
	
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
